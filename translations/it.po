#: dist/prefs.js:1115
msgid "Import layouts from a file"
msgstr "Importa layouts da un file"

#: dist/prefs.js:1118
msgid "Select layouts file"
msgstr "Seleziona file di layouts"

#: dist/prefs.js:1121 dist/prefs.js:1493
